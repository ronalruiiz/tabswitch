{
  "manifest_version": 3,
  "name": "Tab Switch",
  "description": "Cambiar entre pestañas de Google Chrome de manera automática",
  "version": "1.0",
  "author": "ronalruiiz",
  "permissions": [
    "activeTab",
    "tabs",
    "scripting"
    ],
  "background": {
    "service_worker": "background.js"
  },
  "icons": {
    "16": "images/icon-16.png",
    "32": "images/icon-32.png",
    "48": "images/icon-48.png",
    "128": "images/icon-128.png"
  }
}